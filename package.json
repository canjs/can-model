{
  "name": "can-model",
  "version": "3.0.2",
  "description": "can.Model",
  "main": "src/can-model.js",
  "scripts": {
    "preversion": "npm test && npm run build",
    "testee": "testee src/test.html --browsers firefox",
    "test": "npm run jshint && npm run testee",
    "jshint": "jshint src/. --config",
    "document": "bit-docs",
    "build": "node build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canjs/can-model.git"
  },
  "keywords": [
    "CanJS",
    "DoneJS"
  ],
  "author": "Prashant Sharma and Bitovi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/canjs/can-model/issues"
  },
  "homepage": "https://github.com/canjs/can-model#readme",
  "dependencies": {
    "can-construct": "^3.0.4",
    "can-event": "^3.0.1",
    "can-list": "^3.0.1",
    "can-map": "^3.0.3",
    "can-observation": "^3.0.3",
    "can-util": "^3.0.13",
    "jquery": "2.x - 3.x"
  },
  "devDependencies": {
    "can-fixture": "^1.0.10",
    "bit-docs": "0.0.7",
    "jshint": "^2.9.1",
    "steal": "^1.5.2",
    "steal-qunit": "1.0.1",
    "steal-tools": "^1.3.3",
    "testee": "^0.7.0"
  },
  "bit-docs": {
    "dependencies": {
      "bit-docs-glob-finder": "^0.0.5",
      "bit-docs-dev": "^0.0.3",
      "bit-docs-js": "^0.0.3",
      "bit-docs-generate-readme": "^0.0.8"
    },
    "glob": {
      "pattern": "**/*.{js,md}",
      "ignore": "node_modules/**/*"
    },
    "readme": {
      "apis": "./docs/apis.json"
    },
    "parent": "can-model"
  }
}
